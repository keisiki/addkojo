@M_KOJO_MESSAGE_COM_K14_300_1
;-------------------------------------------------
;기입체크 (=0, 비표시、1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
	IF FLAG:시간정지
	ELSEIF FLAG:역극 == [[대요정]] && !CFLAG:14:1002
	;大妖精に限り初対面口上へ移行
		CALL M_KOJO_ENCOUNTER_K14
	ELSEIF TCVAR:302 && !TALENT:연모 && !TALENT:사모 && CFLAG:14:1002 != 1
		SELECTCASE RAND:3
			CASE 1
				CALL COLORMESSAGE14("　에, 에취잇!",C_GREEN,1,0)
				PRINTFORMW 「후…나의 근처에 있으면 요정들도 동상에 걸린다고」
			CASE 0
				PRINTFORML 「근데 넌 누구야?」
				CALL COLORMESSAGE14("(아직도 이름을 기억하지 못하고 있어……）",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「…아까부터 유달리 들이대고 있네?」
				CALL COLORMESSAGE14("　죄, 죄송합니다!",C_GREEN,2,0)
		ENDSELECT
	ELSEIF CFLAG:318 == 1
		PRINTFORML 「………」
	ELSEIF CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 2 || CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 202
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「아이스 사러 왔어? 하나에 500엔이야」
				IF MONEY < 500
					CALL COLORMESSAGE14("　으으…돈이 부족해….",C_GREEN,2,0)
				ELSE
					CALL COLORMESSAGE14("　아이스, 맛있어 보여…. 돈도 있으니 사먹을까?",C_GREEN,2,0)
				ENDIF
			CASE 0
				PRINTFORML 「지금 일하는 중이야. 너무 바빠서 정신이 없네」
				PRINTFORMW 「인기가 있어도 문제라니까-」
				CALL COLORMESSAGE14("（정말 바빠 보여…. 도와주는 게 좋을려나?)",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「태양 저 망할 녀석이 나의 아이스를 녹이려고 하고 있어…」
				PRINTFORMW 「우오옷-! 질까 보냐!」
				CALL COLORMESSAGE14("　힘내 치르노짱!",C_GREEN,2,0)
		ENDSELECT
	;ELSEIF TALENT:연모 
		;PRINTFORML 
	ELSEIF TALENT:사모 || CFLAG:14:1002 == 1
		SELECTCASE RAND:8
			CASE 6
				PRINTFORML 「흉기로는 나이프보다 얼음쪽이 더 좋아!」
				PRINTFORML 「얼음 나이프는 녹아버리면 증거가 인멸되니까」
				CALL COLORMESSAGE14("　치르노짱. 혹시 바깥세계의 탐정만화 읽었어?",C_GREEN,1,0)
				CALL COLORMESSAGE14("「뭐야. 다이짱도 읽었구나」",C_AQUA,2,0)
			CASE 5
				CALL COLORMESSAGE14("「다이짱은 무슨 요정?」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　에? 나? 나는……무슨 요정일까나.",C_GREEN,1,0)
				CALL COLORMESSAGE14("「라바 같은 나비의 요정?」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　하지만 동면은 안 하는걸. …으-음, 나는 도대체.",C_GREEN,1,0)
				PRINTFORMW 「…뭔가 이변이 일어날 것만 같은 예감」
				CALL COLORMESSAGE14("　내 정체로 이변은 일어나진 않는다고.",C_GREEN,2,0)
			CASE 4
				PRINTFORMW 「또 피부 그을릴수 있으려나-. 요즘의 태양은 연약해서 안 돼」
				CALL COLORMESSAGE14("　그 때의 치르노짱, 정말 강해서 멋있었어",C_GREEN,2,0)
			CASE 3
				PRINTFORMW 「카라스텐구는 조심하는 게 좋아. 있는 일 없는 일 전부 기사화 되버리니까!」
				CALL COLORMESSAGE14("　큰두꺼비씨의 기사 같은…?",C_GREEN,1,0)
				CALL COLORMESSAGE14("「그래! 그 신문은 거짓말 뿐. 저녀석은 큰두꺼비 상대로 좀 더 선전했었다고」",C_AQUA,2,0)
			CASE 2
				PRINTFORML 「어디 개구리 없으려나」
				CALL COLORMESSAGE14("　또 개구리를 얼리려는거야? 불쌍하니까 그만두자….",C_GREEN,1,0)
				CALL COLORMESSAGE14("「제대로 소생시키니까 괜찮대도」",C_AQUA,2,0)
			CASE 1
				PRINTFORML 「아-뭔가 평화로운걸-.  또 이변이라도 일어나지 않으려나-」
				CALL COLORMESSAGE14("　평화로운 게 최고야 치르노짱.",C_GREEN,1,0)
				PRINTFORMW 「나에겐 실컷 활개치며 놀 수 있는 이변이 일어나는 쪽이 매일 즐거운걸」
			CASE 0
				PRINTFORML 「초원이나 자갈길에서 주위를 얼리고 뒹굴면 즐겁다고」
				PRINTFORMW 「미끌미끌 미끄러져 굴러가고… 정-말 재밌어-!」
			CASEELSE
				PRINTFORML 「봐, 꽃잎 얼리면 멋있다고!」
				CALL COLORMESSAGE14("　드라이플라워라도 만들게?",C_GREEN,1,0)
				PRINTFORMW 「이렇게 얼리면 흉기가 된다고!」
				CALL COLORMESSAGE14("　와, 와아….",C_GREEN,2,0)
		ENDSELECT
	ELSE
		SELECTCASE RAND:6
			CASE 4
				PRINTFORML 「나는 여름 맑은 날에만, 인간 마을에서 아이스를 판다고」
				PRINTFORMW 「아이스를 한여름 태양 밑에서 파는 기술은 나에게만 가능하니까 짭짤하다고!」
				CALL COLORMESSAGE14("　저도 아이스는 정말 좋아해요. 혹시 만난다면 꼭 팔아주세요.",C_GREEN,2,0)
				SIF DAY:2 == 4
					CALL COLORMESSAGE14("　겨울 아이스도 맛있을 것 같지만, 감기 걸릴것 같아….",C_GREEN,2,0)
			CASE 3
				PRINTFORML 「신사 근처의 삼요정은 대단하다고」
				PRINTFORML 「서니는 기척을 보이지 않게 하고, 루나는 빛을 없애고, 스타는… 놀랍게도 소리를 감지할 수 있어!」
				PRINTFORMW 「분하지만, 그녀석들을 숨바꼭질에서 능가할 수 있는 녀석은 없을껄」
				CALL COLORMESSAGE14("（어라…뭔가 다른 듯 한?)",C_GREEN,2,0)
			CASE 2
				IF DAY:2 == 4
					PRINTFORMW 「겨울은 지루하네에. 아무것도 안 해도 주변은 얼어있고」
					CALL COLORMESSAGE14("　벌레나 여름의 요정들도 겨울잠을 자서 놀지 못하니까, 겨울은 좀 쓸쓸하네",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「자, 오늘은 무엇을 얼려버릴까나」
					CALL COLORMESSAGE14("　저, 저는 얼리지 말아주세요….",C_GREEN,2,0)
				ENDIF
			CASE 1
				PRINTFORML 「그런데 너 이름이 뭐였더라?」
				CALL COLORMESSAGE14("　제 이름이요? 저는…",C_GREEN,1,0)
				PRINTFORMW 「외우기 힘드니까 그대로 「너」라고 부를께」
				CALL COLORMESSAGE14("　그런….",C_GREEN,2,0)
			CASE 0
				PRINTFORML 「왠지 얌전한 녀석이네-. 밥 먹는 거라도 잊었어?」
				CALL COLORMESSAGE14("　제, 제대로 먹고 있어요",C_GREEN,1,0)
				IF TALENT:MASTER:가슴사이즈 > -1
					CALL COLORMESSAGE14("「…잘 안먹으면 저렇게 커지진 않을테니까」",C_AQUA,1,0)
					CALL COLORMESSAGE14("　저, 저기…어디를 보고 계신거죠?",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「정말? 풀만 먹는 건 아니고?」
				ENDIF
			CASEELSE
				PRINTFORML 「너는 숨바꼭질이 뭔지 알아?」
				CALL COLORMESSAGE14("　귀신이 숨어있는 아이를 찾는 놀이…맞죠? 태양의 밭의 요정들과 자주 즐기고 있어요.",C_GREEN,1,0)
				PRINTFORML 「호수의 요정들 사이에서도 유행중이야」
				PRINTFORML 「그리고 항상 내가 혼자서 이긴다고. 엣헴!」
				PRINTFORML 「맞아! 다음에는 할 때는 너도 끼워줄께」
				CALL COLORMESSAGE14("　제가 치르노씨를 찾을 수 있을까…. 열심히 해보겠습니다.",C_GREEN,2,0)
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
