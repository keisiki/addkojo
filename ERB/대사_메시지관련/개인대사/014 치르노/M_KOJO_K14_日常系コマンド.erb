								CALL COLORMESSAGE14("「낚을 수 있어」",C_AQUA,1,0)
								CALL COLORMESSAGE14("　그, 그렇구나….",C_GREEN,2,0)
							CASEELSE
								CALL COLORMESSAGE14("　……낚이지 않네.",C_GREEN,1,0)
								PRINTFORMW 「물고기놈, 잘도 이쪽을 우습게 봤겠다…!」
						ENDSELECT
					CASE 1 TO 4
