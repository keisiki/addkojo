			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「아, 이 차 식어버렸네」
					CALL COLORMESSAGE14("　그렇다면 치르노짱도 맛을 수 있겠네.",C_GREEN,1,0)
					PRINTFORMW 「더 차갑게 해주지」
					CALL COLORMESSAGE14("　찻잎이 얼어서 떠올라 버렸어….",C_GREEN,2,0)
				CASE 0
					CALL COLORMESSAGE14("　치르노짱은 홍차에 설탕 몇 개 넣어?",C_GREEN,1,0)
					PRINTFORMW 「나는 설탕 3개 넣-어줘」
					CALL COLORMESSAGE14("　3개? 단 거 3개 원하나…. 욕심쟁이네.",C_GREEN,2,0)
				CASEELSE
					PRINTFORML 「차 맛있어-」샤샥
					CALL COLORMESSAGE14("　치르노짱, 그 막대 아이스는 혹시…",C_GREEN,1,0)
					PRINTFORMW 「받은 차를 아이스캔디로 만들어봤어」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「써! 한잔 더-!」
					CALL COLORMESSAGE14("　저어…조금 녹아버린 것 같은데 괜찮으신가요?",C_GREEN,2,0)
				CASE 0
					PRINTFORML 「뜨거운 차는… 이렇게!」
					CALL COLORMESSAGE14("　와… 아무것도 없는 곳에서 얼음이…!",C_GREEN,1,0)
					PRINTFORML 「훗, 최강이지」
					PRINTFORML 「…써!」
					CALL COLORMESSAGE14("　다음에는 설탕도 준비해 놓을게요",C_GREEN,2,0)
				CASEELSE
					CALL COLORMESSAGE14("　저, 치르노씨. ㅊ, 차 한잔 드실래요?",C_GREEN,1,0)
					PRINTFORML 「…나에게 뜨거운 음료를 건네다니 괴롭히는거야?」
					CALL COLORMESSAGE14("　아…! 죄송합니다….",C_GREEN,1,0)
					PRINTFORML 「뭐, 이정도는 마시지 않으면 최강의 이름이 아깝지!」
					PRINTFORMW 「뜨것!」
			ENDSELECT
		ENDIF
	RETURN 1
ENDIF
