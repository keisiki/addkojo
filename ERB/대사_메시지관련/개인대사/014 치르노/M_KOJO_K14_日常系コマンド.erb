@M_KOJO_MESSAGE_SCOM_K14_80_1
;-------------------------------------------------
;기입체크 (=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
		IF FLAG:시간정지
		ELSEIF CFLAG:318 == 1
		;ELSEIF TALENT:연모
			;PRINTFORML 
		ELSEIF TALENT:사모 || CFLAG:14:1002 == 1
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「이왕이니까 빨리 가자!」
					CALL COLORMESSAGE14("　좋아, 지지 않겠어.",C_GREEN,2,0)
				CASE 0
					PRINTFORML 「......」데굴데굴데굴데굴
					CALL COLORMESSAGE14("　치르노짱, 도대체 왜 몸을 돌리는 거야?",C_GREEN,1,0)
					PRINTFORMW 「급할수록 돌아가라고 하니까, 세로로 돌아봤어.」
				CASEELSE
					CALL COLORMESSAGE14("「오옷? 오늘따라 적극적인 것 같네, 다이짱!」",C_AQUA,1,0)
					PRINTFORMW 「그렇다면 이 몸도 질 수는 없지!」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「오, 빨리 가기 승부라는 거지?」
					PRINTFORMW 「이 승부도 이 몸이 이겨주겠어!」
				CASE 0
					PRINTFORMW 「화급한 용무가 있는 거야?」
				CASEELSE
					PRINTFORMW 「이 몸의 손을 잡다니...배짱이 두둑하구나」
			ENDSELECT
		ENDIF
ENDIF
RESETCOLOR
RETURN 1