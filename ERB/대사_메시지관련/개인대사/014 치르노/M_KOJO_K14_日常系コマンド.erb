;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
		LOCALS = %ITEMNAME:(TFLAG:193)%
		IF FLAG:시간정지
		ELSEIF CFLAG:318 == 1
			PRINTFORML 
		ELSEIF LOCALS == "개구리" 
		;カエルを釣るとチルノが欲しがる。何個もあげると何かもらえる。
			PRINTFORMW 「아, 개구리! 그거 갖고 싶어!」
			PRINTFORML
			PRINTFORML  
			PRINTFORMW 　치르노에게 개구리를 줄까?
			CALL ASK_YN
			IF !RESULT
				CALL LOST_ITEM("개구리",1,2)
				CALL COLORMESSAGE14("　좋아요. 소중히 아껴주세요",C_GREEN,1,0)
				PRINTFORMW 「와-! 고마워!」
				CFLAG:14:1003 ++
				SELECTCASE CFLAG:14:1003
					CASE 5,10,15
						PRINTFORML 「항상 개구리 주니까, 답례로 이거 줄게」
						CALL COLORMESSAGE14("　예쁘다…. 정말로 받아도 되는 거야?",C_GREEN,1,0)
						CALL COLORMESSAGE14("「괜찮다니까-!」",C_AQUA,1,0)
						CALL COLORMESSAGE14("　고마워…. 소중히 할께.",C_GREEN,2,0)
						CALL GET_ITEM("얼음의비늘",1,2)
					CASEELSE
				ENDSELECT
			ELSE
				CALL COLORMESSAGE14("　미안해. 나도 개구리가 갖고 싶어서….",C_GREEN,1,0)
				PRINTFORMW 「에 짠돌이!」
			ENDIF
		ELSEIF TALENT:연모 || TALENT:사모 || CFLAG:14:1002 == 1
			IF TFLAG:194 > 1000
			;チルノが釣った
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「나를 고생시키다니, 이 놈도 꽤나 하는걸」
