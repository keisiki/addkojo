@M_KOJO_SUCCESS_COM_K14_301
;성공실패 판정
;TFLAG:192 (-2=커맨드 종료, -1＝강제실패, 0=COM의존, 1=강제성공or대성공)
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K14_301
CALL M_KOJO_MESSAGE_COM_K14_301_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K14_301_1
;-------------------------------------------------
;기입체크 (=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
		IF FLAG:시간정지
		ELSEIF CFLAG:318 == 1
		;ELSEIF TALENT:연모
			;PRINTFORML 
		ELSEIF TALENT:사모 || CFLAG:14:1002 == 1
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「아, 차가 식어버렸네」
					CALL COLORMESSAGE14("　그렇다면 치르노짱도 먹을 수 있겠네.",C_GREEN,1,0)
					PRINTFORMW 「더욱 더 얼려주지!」
					CALL COLORMESSAGE14("　차, 찻잎이 얼어서 떠올랐어....",C_GREEN,2,0)
				CASE 0
					CALL COLORMESSAGE14("　치르노짱은 홍차에 각설탕 몇 개 넣어?",C_GREEN,1,0)
					PRINTFORMW 「이 몸은 3개 넣어줘」
					CALL COLORMESSAGE14("　3개나? 너무 많은 거 아냐?....이가 썩겠어.",C_GREEN,2,0)
				CASEELSE
					PRINTFORML 「차 맛있어~」샤샥
					CALL COLORMESSAGE14("　치르노짱, 그 막대 아이스는 혹시...",C_GREEN,1,0)
					PRINTFORMW 「받은 차를 아이스로 만들어봤지. 어때?」
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					PRINTFORML 「써! 한잔 더-!」
					CALL COLORMESSAGE14("　저어...조금 녹아버린 것 같은데, 괜찮으신가요?",C_GREEN,2,0)
				CASE 0
					PRINTFORML 「차가 너무 써...그렇다면!」
					CALL COLORMESSAGE14("　우왓, 찻잔에 얼음이..!",C_GREEN,1,0)
					PRINTFORML 「훗, 이게 바로 최강의 힘이라고.」
					PRINTFORML 「...써!」
					CALL COLORMESSAGE14("　다음번에는 설탕도 준비할게요",C_GREEN,2,0)
				CASEELSE
					CALL COLORMESSAGE14("　저, 치르노씨. ㅊ, 차 한잔 드실래요?",C_GREEN,1,0)
					PRINTFORML 「...이 몸에게 뜨거운 음료를 내놓다니, 도전하는 거야?」
					CALL COLORMESSAGE14("　아...! 죄송합니다....",C_GREEN,1,0)
					PRINTFORML 「뭐, 이 정도 도전 따위는 그냥 받아주지. 최강의 이름에 먹칠할 수는 없으니!」
					PRINTFORMW 「..너무 써!」
			ENDSELECT
		ENDIF
	RETURN 1
ENDIF
RESETCOLOR
RETURN 1
