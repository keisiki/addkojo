			ENDSELECT
		ELSE
			SELECTCASE RAND:3
				CASE 1
					CALL COLORMESSAGE14("　흐앗....",C_GREEN,1,0)
					CALL COLORMESSAGE14("「오- 너 날개가 되게 보송보송하네」",C_AQUA,1,0)
					PRINTFORMW 「얼려봐도 될까?」
					CALL COLORMESSAGE14("　아, 안 돼요!!",C_GREEN,2,0)
				CASE 0
					CALL COLORMESSAGE14("　아, 머리 흐트러졌다. 고쳐드릴게요",C_GREEN,1,0)
					CALL COLORMESSAGE14("「응, 맡길게-」",C_AQUA,1,0)
					CALL COLORMESSAGE14("　으으...곱슬머리라서 그런지 보들보들해...정말 부러워요.",C_GREEN,1,0)
					PRINTFORMW 「네 옆에 묶인 머리도 달랑거리는 게 왠지 재밌어 보여」
				CASEELSE
					CALL COLORMESSAGE14("　치르노씨는 몸도 차갑네요.",C_GREEN,1,0)
					CALL COLORMESSAGE14("「흥, 너 바보구나?」",C_AQUA,1,0)
					PRINTFORMW 「얼음 요정님의 몸은 항상 차갑게 식어있다고!」
			ENDSELECT
		ENDIF
ENDIF
RESETCOLOR
RETURN 1
