					CASE 0
						PRINTFORML 「으-음, 최강이네. 이제 뭐든 낚을 수 있을 것 같아」
						PRINTFORML 「줄줄이 낚을 수 있을것만 같은 기분-!」
						CALL COLORMESSAGE14("　다음에는 뭐가 낚일까?",C_GREEN,2,0)
					CASEELSE
						PRINTFORML 「오, 오오오…?」
						CALL COLORMESSAGE14("　치르노짱. 당겨! 당겨!",C_GREEN,1,0)
						PRINTFORML 「내가 질까보냐! 크으으으윽!」
						CALL COLORMESSAGE14("　아, 보이기 시작했어! 힘 내!",C_GREEN,1,0)
						PRINTFORML 첨벙!
						PRINTFORMW 「낚았다! 어때! 손들었냐!」
						CALL COLORMESSAGE14("　과연 치르노짱!",C_GREEN,2,0)
				ENDSELECT
			ELSE
				SELECTCASE TFLAG:193
					CASE 0 
