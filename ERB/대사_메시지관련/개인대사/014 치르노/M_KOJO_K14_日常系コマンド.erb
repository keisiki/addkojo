@M_KOJO_SUCCESS_COM_K14_300
;성공실패 판정
;TFLAG:192 (-2=커맨드 종료, -1＝강제실패, 0=COM의존, 1=강제성공or대성공)
TFLAG:192 =0

@M_KOJO_MESSAGE_COM_K14_300
;커맨드 공통 메시지(TRAIN_MESSAGE) 
;삭제하면 「~이야기를 했다.」 이런 것들이 사라집니다. 또, 삭제했을 경우는 자동으로 구상색이 붙지 않습니다.
CALL TRAIN_MESSAGE
;구상메인호출
CALL M_KOJO_MESSAGE_COM_K14_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K14_300_1
;-------------------------------------------------
;기입체크 (=0, 비표시, 1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
	IF FLAG:시간정지
	ELSEIF FLAG:역극 == [[대요정]] && !CFLAG:14:1002
	;大妖精に限り初対面口上へ移行
		CALL M_KOJO_ENCOUNTER_K14
	ELSEIF TCVAR:302 && !TALENT:연모 && !TALENT:사모 && CFLAG:14:1002 != 1
		SELECTCASE RAND:3
			CASE 1
				CALL COLORMESSAGE14("　흐, 흐앗!",C_GREEN,1,0)
				PRINTFORMW 「후...이 몸의 근처에 있으면 요정들도 동상에 걸린다고!」
			CASE 0
				PRINTFORML 「근데 넌 이름이 뭐야?」
				CALL COLORMESSAGE14("(아직도 이름을 기억하지 못하고 있어......）",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「...아까부터 계속 덤비는데, 최강을 이길 자신이 있어서 그러는 거야?」
				CALL COLORMESSAGE14("　죄, 죄송합니다!",C_GREEN,2,0)
		ENDSELECT
	ELSEIF CFLAG:318 == 1
		PRINTFORML 「.........」
	ELSEIF CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 2 || CFLAG:행동 == 5 && CFLAG:MASTER:현재위치 == 202
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「아이스 먹을래? 하나에 500원인데」
				IF MONEY < 500
					CALL COLORMESSAGE14("　으으...돈이 부족해....",C_GREEN,2,0)
				ELSE
					CALL COLORMESSAGE14("　아이스, 맛있어 보여....돈도 있으니 하나 사먹을까?",C_GREEN,2,0)
				ENDIF
			CASE 0
				PRINTFORML 「지금 일하는 중인데, 너무 바빠서 눈이 핑핑 돌고 있어」
				PRINTFORMW 「인기가 너무 많아도 문제라니까~」
				CALL COLORMESSAGE14("（너무 바빠 보여....도와주는 게 좋겠지?)",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「태양빛이 너무 강해서 이 몸의 아이스가 녹아버리고 있어...」
				PRINTFORMW 「우오옷! 태양빛 따위에게 질 수는 없지-!」
				CALL COLORMESSAGE14("　힘내요, 치르노씨!",C_GREEN,2,0)
		ENDSELECT
	;ELSEIF TALENT:연모 
		;PRINTFORML 
	ELSEIF TALENT:사모 || CFLAG:14:1002 == 1
		SELECTCASE RAND:8
			CASE 6
				PRINTFORML 「흉기로는 나이프보다 얼음이 더 좋아!」
				PRINTFORML 「그대로 녹아버리면 증거인멸도 완벽하잖아!」
				CALL COLORMESSAGE14("　치르노짱. 혹시 외부에서 들어왔다는 그 탐정 만화 읽었어?",C_GREEN,1,0)
				CALL COLORMESSAGE14("「뭐야, 다이짱도 읽었구나」",C_AQUA,2,0)
			CASE 5
				CALL COLORMESSAGE14("「다이짱은 무슨 요정이야?」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　에, 나? 나는..그러니까...",C_GREEN,1,0)
				CALL COLORMESSAGE14("「라바짱 같은 나비 요정?」",C_AQUA,1,0)
				CALL COLORMESSAGE14("　하지만 겨울잠은 안 자는걸....으응, 나는 무슨 요정인걸까",C_GREEN,1,0)
				PRINTFORMW 「...뭔가 이변이 일어날 것만 같은 예감이!」
				CALL COLORMESSAGE14("　고작 내 정체 가지고 이변이 일어나지는 않을걸?",C_GREEN,2,0)
			CASE 4
				PRINTFORMW 「요즘은 태양빛이 너무 약해. 또 그을렸으면 좋겠는데」
				CALL COLORMESSAGE14("　그 때의 치르노짱은 무녀나 마법사보다도, 텐구보다도 더 멋졌어.",C_GREEN,2,0)
			CASE 3
				PRINTFORMW 「카라스 텐구는 조심하는 게 좋아. 없는 일도 있는 일마냥 기사로 적어버리니까!」
				;놀랍게도 치르노는 요미가나나 루비가 전혀 없는 한자 투성이의 붕붕마루 신문도 읽을 수가 있다.
				CALL COLORMESSAGE14("　...신문인데, 그래도 되는 거야?",C_GREEN,1,0)
				CALL COLORMESSAGE14("「그러니까 말이야! 특히 저 신문이 그래. 저 녀석이 쓰는 신문은 악의가 가득하다고!」",C_AQUA,2,0)
			CASE 2
				PRINTFORML 「개구리가 어디 있으려나-」
				CALL COLORMESSAGE14("　이제 그만 얼리자. 개구리가 불쌍해....",C_GREEN,1,0)
				CALL COLORMESSAGE14("「잠깐 뒤에 다시 소생시켜주니까 괜찮아!」",C_AQUA,2,0)
			CASE 1
				PRINTFORML 「어쩐지 너무 평화로운걸... 이변이라도 일어나지 않으려나」
				CALL COLORMESSAGE14("　평화로운 게 최고야, 치르노짱.",C_GREEN,1,0)
				PRINTFORMW 「이 몸에겐 실컷 활개치며 놀 수가 있는 이변이 최고인걸!」
			CASE 0
				PRINTFORML 「초원이나 자갈길을 얼리고 뒹구는 건 재밌어」
				PRINTFORML 「미끌미끌 미끄러져서...흐앗!」
			CASEELSE
				PRINTFORML 「봐, 이번에는 꽃잎을 얼려볼거야!」
				CALL COLORMESSAGE14("　드라이플라워라도 만들게?",C_GREEN,1,0)
				PRINTFORMW 「이렇게 하면 흉기가 되거든! 꽃잎수리검이다-!」
				CALL COLORMESSAGE14("　와, 와아....",C_GREEN,2,0)
		ENDSELECT
	ELSE
		SELECTCASE RAND:6
			CASE 4
				PRINTFORML 「이 몸은 여름의 맑은 날에만 아이스를 판다고.」
				PRINTFORMW 「태양 아래에서 파는 아이스! 이 몸은 부자가 될 거야-!」
				CALL COLORMESSAGE14("　저도 아이스는 좋아해요, 다음에 만나면 꼭 팔아주세요.",C_GREEN,2,0)
				SIF DAY:2 == 4
					CALL COLORMESSAGE14("　겨울 아이스도 괜찮을 것 같지만...추워서 감기에 걸릴 것 같아요....",C_GREEN,2,0)
			CASE 3
				PRINTFORML 「신사 주변의 세 요정은 굉장해」
				PRINTFORML 「서니는 기척을 숨기고, 루나는 빛을 없애고, 스타는...소리를 지울 수 있어!」
				;서니는 맞았지만, 루나랑 스타는 반대로 서술했다. 이 바보 요정...
				PRINTFORMW 「짜증나지만, 그 녀석들을 숨바꼭질에서 이기기는 이 몸에게도 힘들어」
				CALL COLORMESSAGE14("（어라...뭔가 능력이 뒤바뀐 것 같은데?)",C_GREEN,2,0)
			CASE 2
				IF DAY:2 == 4
					PRINTFORMW 「겨울은 재미없어. 내가 안 해도 이미 주변이 얼어있고...」
					CALL COLORMESSAGE14("　다른 해바라기나 벌레 요정들은 다 겨울잠을 자니까 말이지",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「자, 오늘은 무엇을 얼려버릴까나~」
					CALL COLORMESSAGE14("　흐앗! 저, 저는 얼리지 말아주세요....",C_GREEN,2,0)
				ENDIF
			CASE 1
				PRINTFORML 「그러고 보면 네 이름이 뭐였더라?」
				CALL COLORMESSAGE14("　제 이름이요? 그러니까, 저는...",C_GREEN,1,0)
				PRINTFORMW 「외우기 힘드니까 그냥 「너」로 칭해도 될까?」
				CALL COLORMESSAGE14("　...저기, 그...아니에요...",C_GREEN,2,0)
			CASE 0
				PRINTFORML 「왠지 요정 치고는 얌전한 녀석이네. 밥 먹는 걸 잊은 거야?」
				CALL COLORMESSAGE14("　자, 잘 먹고 있어요",C_GREEN,1,0)
				IF TALENT:MASTER:가슴사이즈 > -1
					CALL COLORMESSAGE14("「...역시, 나도 잘 먹어야 커지려나...?」",C_AQUA,1,0)
					CALL COLORMESSAGE14("　저, 저기...어디를 보고 계신거죠?",C_GREEN,2,0)
				ELSE
					PRINTFORMW 「정말? 풀만 먹는 게 아니고?」
				ENDIF
			CASEELSE
				PRINTFORML 「너는 숨바꼭질이 뭔지 알아?」
				CALL COLORMESSAGE14("　귀신이 숨어있는 존재를 찾는 놀이...맞죠? 태양밭의 요정들과 자주 즐기고 있어요.",C_GREEN,1,0)
				PRINTFORML 「호수의 요정들 사이에서도 유행중이야」
				PRINTFORML 「그리고 이 몸이 바로 1인자란 말이지. 엣헴!」
				PRINTFORML 「좋았어, 다음에는 너도 같이 하자」
				CALL COLORMESSAGE14("　제, 제가 과연 치르노씨를 찾을 수 있을까요?....열심히 해보겠습니다.",C_GREEN,2,0)
				PRINTFORMW 
		ENDSELECT
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
