;기입체크 (=0, 비표시、1, 표시)
LOCAL = 1
;-------------------------------------------------
IF LOCAL
SETCOLOR C_AQUA
	IF FLAG:시간정지
	ELSEIF CFLAG:318 == 1
		PRINTFORMW 
	;ELSEIF TALENT:연모
		;PRINTFORML 
	ELSEIF TALENT:사모 || CFLAG:14:1002 == 1
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「아, 왕건이다」
				CALL COLORMESSAGE14("　그, 그렇게 보여주지 마…부끄러워….",C_GREEN,1,0)
				PRINTFORMW 「모두에게 보여주자! 으쌰-!」
				CALL COLORMESSAGE14("　정말 그만둬어…!",C_GREEN,2,0)
				
			CASE 0
				PRINTFORML 「굉장해…이렇게나 깊이 들어간다니」
				CALL COLORMESSAGE14("　싫엇…、치르노짱의 것이 내 안에…읏, 무, 무서워…",C_GREEN,1,0)
				PRINTFORMW 「얼굴 붉게 물들여놓고…, 실은 기분 좋지?」
				CALL COLORMESSAGE14("　아, 안 돼에…! 거기는 민감한데… 망가져버렷…!",C_GREEN,2,0)
			CASEELSE
				PRINTFORML 「으-응 나오질 않아」
				CALL COLORMESSAGE14("(치르노짱의 무릎 매끈매끈…）",C_GREEN,2,0)
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 1
				PRINTFORML 「볼이 떨어지지 않을까. 귀가 동상에 걸릴까…. 어느 쪽이 빠를까나?」
				PRINTFORMW 
			CASE 0
				PRINTFORML 샤라라~♪ 샤라라라라､라､라､라､～♪
				PRINTFORMW 「귀청소한 수라면, 이쪽이 위라고?」
			CASEELSE
				PRINTFORML 「이 막대기를 써서? 음음 그렇군…」
				PRINTFORMW 「으럇!」푸욱
		ENDSELECT
	ENDIF
ENDIF
RESETCOLOR
RETURN 1
