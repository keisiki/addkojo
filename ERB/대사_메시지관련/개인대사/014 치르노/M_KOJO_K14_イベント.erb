				;PRINTFORML 
			ELSEIF TALENT:사모
				SELECTCASE RAND:3
					;CASE 1
						;PRINTFORML 
						;PRINTFORMW 
					;CASE 0
						;PRINTFORML 
						;PRINTFORMW 
					CASEELSE	
						CALL COLORMESSAGE14("다이짱-! 더 놀자!",C_AQUA,2,0)
