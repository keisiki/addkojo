						CALL COLORMESSAGE14("오늘도 같이 놀아줘서 고마워요, 치르노씨.",C_GREEN,2,0)
						CALL COLORMESSAGE14("「뭐, 최강에게 이 정도는 별 것도 아니지!」",C_AQUA,2,0)
					CASEELSE
						PRINTFORML 「오늘도 이 몸의 최강 전설을 환상향에 널리 떨쳤다!」
						PRINTFORMW 「내일은 더 많이 떨쳐주겠어!」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;반발각인 소거 이벤트
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 1
			PRINTFORML 「아, 잘 놀았다! 또 놀자!」
			CALL COLORMESSAGE14("　...치르노짱.",C_GREEN,2,0)
			CALL COLORMESSAGE14("　이런 짓을 해놓고 용서 받을 수 있을 리가 없지만...",C_GREEN,2,0)
			CALL COLORMESSAGE14("　...미안해.",C_GREEN,2,0)
			PRINTFORML 「...별로, 이 몸은 화 안 났어!」
			CALL COLORMESSAGE14("　...",C_GREEN,2,0)
			PRINTFORML 「놀다보면, 그 정도 화는 그냥 날아가버린다구!」
			CALL COLORMESSAGE14("　으으...정말로 미안해, 치르노짱...",C_GREEN,2,0)
			CALL COLORMESSAGE14("「괜찮으니까, 다음에 또 놀자! 그거면 충분해!」",C_AQUA,2,0)
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;반발각인2>1 레벨 다운 이벤트
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 2
			PRINTFORML 「이 몸의 마음은 빙산의 일각처럼 넓다고. 그러니, 이제 용서해주겠...」
			PRINTFORMW 「...다고 할 줄 알았냐! 이 몸의 분노의 얼음탄이나 먹어버려!」
			CALL COLORMESSAGE14("　꺄아앗?!",C_GREEN,2,0)
			CALL COLORMESSAGE14("(...으응? 그렇게 얼음탄이 크지가 않아...）",C_GREEN,2,0)
			PRINTFORMW 「...한번만 더 그랬다간 얼음 동상으로 만들어버릴거다. 각오해!」
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;반발각인3>2 레벨 다운 이벤트
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 3
			;PRINTFORML 
			RETURN 1
		ENDIF
		PRINTFORML
ENDIF
RESETCOLOR
RETURN 1

;※※※※※※※※※※※※※※※※※※※※※※※※※※
;"EVENT"계 이벤트
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;방에서 조우
;==================================================
@M_KOJO_EVENT_K14_1(ARG,ARG:1)
;구상주는 TARGET
;--------------------------------------------------
;기입체크 (=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
SETCOLOR C_AQUA
		;-------------------------------------------------
		;소분기
		;-------------------------------------------------
		;MASTER가 들어왔다
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 1
			IF FLAG:시간정지
			ELSEIF CFLAG:318 == 1
				PRINTFORML 
			;ELSEIF TALENT:연모
				;PRINTFORML 
			ELSEIF TALENT:사모
				SELECTCASE RAND:4
					CASE 2
						CALL COLORMESSAGE14("「다이짱!」",C_AQUA,1,0)
						CALL COLORMESSAGE14("　앗, 치르노짱!",C_GREEN,1,0)
						PRINTFORML 「놀러와준거야?」
					CASE 1
						PRINTFORML 「빈틈 발견!」
						CALL COLORMESSAGE14("　히약!",C_GREEN,1,0)
						PRINTFORML 「어때? 놀랐어?」
					CASE 0
						PRINTFORML 「오, 놀이 상대 발견!」
					CASEELSE
						CALL COLORMESSAGE14("「다이짱, 이걸 봐!」",C_AQUA,1,0)
						PRINTFORMW 「왠지 여기 있을 것 같았어...」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「후후훗. 찾았다! 이제 네가 술래야!」
					CASE 0
						PRINTFORML 「응? 뭐야, 너였어? 놀래키지 말라고」
					CASEELSE
						CALL COLORMESSAGE14("　아, 치르노씨!",C_GREEN,1,0)
						PRINTFORML 「...아까부터 눈치채고 있었다고?」
						PRINTFORML 「네 놈, 보고 있구나!」
						CALL COLORMESSAGE14("(　...? 나를 바라보고 있지만, 나에게 하는 말은 아냐...도대체 뭘까?)",C_GREEN,1,0)
						;디오 패러디로 적당히 바꿔봄. 원문은 狸の置物に話しかけて何してるのかな?임
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGET이 들어왔다
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 2
		;치르노の部屋で遭遇
			IF CFLAG:14:현재위치 == 345
				IF CFLAG:318 == 1
					PRINTFORML 
				;ELSEIF TALENT:연모
					;PRINTFORML 
				ELSEIF TALENT:사모
					SELECTCASE RAND:3
						CASE 1
							PRINTFORML 「뭐야, 있으면 있다고 말하지. 그랬으면 바로 돌아왔을텐데」
						CASE 0
							PRINTFORM 「이 몸의
							PRINTDATA
								DATAFORM 얼음궁전
								DATAFORM 프리즈 팰리스
								DATAFORM 치르노 이글루-
								DATAFORM ...네가 기억하기 쉽게 줄여서,
								DATAFORM 마경빙정(魔境氷情)!
							ENDDATA
							PRINTFORML 에 온 걸 환영한다!」
							CALL COLORMESSAGE14("(또 이름이 바뀌었어...）",C_GREEN,2,0)
						CASEELSE
								IF DAY:2 == 2
									CALL COLORMESSAGE14("　시원해서 기분 좋아...",C_GREEN,1,0)
									PRINTFORML 「별로, 이 몸은 그런 기분이 느껴지지 않지만」
									CALL COLORMESSAGE14("　아아, 계속 여기 있고 싶어...",C_GREEN,1,0)
								ELSE
									PRINTFORML 「...입술이 파래보이는데, 괜찮아?」
								ENDIF
					ENDSELECT
				ELSE
					SELECTCASE RAND:3
						CASE 1
							PRINTFORML 「...? 이 몸이랑 같이 놀 생각이야?」
							PRINTFORML 「그렇다면 도장깨기를 하러 가자!」
							PRINTFORML 「빈곤한 무녀의 신사를 깨부수는 거야!」
							CALL COLORMESSAGE14("　...도장깨기라고 해서, 정말로 깨부수는 건 아닐텐데요...",C_GREEN,1,0)
							PRINTFORML 
						CASEELSE
							PRINTFORML 「여긴 이 몸의 성이라고. 흙 묻은 발로 들어오면 안 돼」
							CALL COLORMESSAGE14("「하지만 같은 요정이기도 하고...딱 이번 한 번만 용서해주겠어」",C_AQUA,1,0)
					ENDSELECT
				ENDIF
			ENDIF
			IF FLAG:시간정지
			ELSEIF CFLAG:318 == 1
				PRINTFORML
			;ELSEIF TALENT:연모
				;PRINTFORML 
			ELSEIF TALENT:사모
				SELECTCASE RAND:3
					CASE 1
						CALL COLORMESSAGE14("「다이짱! 여기야! 빨리 와!」",C_AQUA,2,0)
						CALL COLORMESSAGE14("　으앗!...",C_GREEN,2,0)
						PRINTFORML 「다이빙 대성공! 헤헤헤, 빠져나오지 못 할거야!」
						CALL COLORMESSAGE14("　그러니까 치르노짱...나 이외에는 가슴에 뛰어들면 안 돼? 위험하니까",C_GREEN,1,0)
					CASE 0
						PRINTFORML 「킥!!」
						PRINTFORMW 피캉-
						CALL COLORMESSAGE14("　꺄아앗!?",C_GREEN,1,0)
						PRINTFORMW 「이상한 모자도 때려잡은 슈퍼 아이스 킥이다!」
						;왠지 치르노가 오키나 걷어차는 아카콘 생각나서 그거 관련으로 재구성했음
					CASEELSE
						PRINTFORML 「당했다! 내 슈퍼 요정 센서에 걸리지 않다니, 대단한 걸?」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「그러고 보면 아직 여기선 놀지 않았네」
					CASE 0
						PRINTFORML 「아, 너! 그러니까, 너에게 볼 일이 있었는데...」
					CASEELSE
						PRINTFORML 「추격전은 끝이다! 빨리 나오시지!」
						PRINTFORML 「...라고 생각했는데, 여기가 아니었나...」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGET이 목욕탕에 들어와서 같이 들어간다
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		;自分用メモ...MASTERと性別が同じ, 時止め中, 恥じらいが０かつ親密が５以上のどれかなら一緒に入る?
		IF LOCAL:1 && ARG == 3
			IF FLAG:시간정지
				RETURN 1
			ELSEIF CFLAG:318 == 1
				PRINTFORML 
			;ELSEIF TALENT:연모
				;PRINTFORML 
			ELSEIF TALENT:사모
				SELECTCASE RAND:3
					;CASE 1
						;PRINTFORML
					CASE 0
						PRINTFORML 「...이 몸도 같이 들어가도 돼?」
						CALL COLORMESSAGE14("　물론이지!",C_GREEN,2,0)
						CALL COLORMESSAGE14("「좋았어!다이짱,씻겨줄게!」",C_AQUA,2,0)
						CALL COLORMESSAGE14("　그럼, 잘 부탁할게",C_GREEN,2,0)
					CASEELSE
						CALL COLORMESSAGE14("　치르노짱.뜨거운 물에서 목욕해도 괜찮아?",C_GREEN,2,0)
						PRINTFORML 「이럴 줄 알고, 무녀에게서 이 패를 받아왔거든!」
						PRINTFORMW 「일시적으로 냉기를 가둬준다고 해. 편리하지?」
						CALL COLORMESSAGE14("　후훗, 그거 좋네. 같이 따뜻해지자...",C_GREEN,2,0)
				ENDSELECT
				
			ELSE
				SELECTCASE RAND:3
					;CASE 1
						;PRINTFORML 
					CASE 0
						PRINTFORML 「오늘은, 이 몸이 목욕탕 물을 극복해보겠어!」
					CASEELSE
						PRINTFORML 「그럼 들어간다!...어라?」
						PRINTFORML 「생각보다는 미지근하네」
						IF TALENT:연모
							PRINTFORMW 「앗, 이 몸이 무심코 차갑게 만들었던 거구나」
							PRINTFORMW 「...미안」
							PRINTFORMW 
						ELSE
							PRINTFORMW 
						ENDIF
				ENDSELECT
			
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGET이 목욕을 하고 나갔다
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		;自分用メモ...態度が３より下, キスかうふふ止まり, 恋慕かセフレではない, なら発生?
		IF LOCAL:1 && ARG == 4
			IF FLAG:시간정지
			ELSEIF CFLAG:318 == 1
				PRINTFORML
			ELSEIF TALENT:사모
				SELECTCASE RAND:3
					;CASE 1
						;PRINTFORML 
						;PRINTFORMW 
					CASE 0
						PRINTFORML 「이 몸은 최강이라는 이름의 신사니까, 이만 자리를 양보해줄게!」
						PRINTFORML 「저, 절대로 덥거나 어지럽거나 그런 건 아니니까 오해하지 말고!」
						PRINTFORMW 
					CASEELSE
						PRINTFORML 「이 몸도 같이 들어가고 싶었는데...」
						PRINTFORMW 
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						;PRINTFORML 
					CASE 0
						PRINTFORML 「으아악! 김에 녹아버리겠어!」
						PRINTFORMW 「오늘이야말로 정복해주겠다고 다짐했지만...철퇴다!」
					CASEELSE
						PRINTFORML 「음...선객인가.」
						CALL COLORMESSAGE14("　미안, 빨리 씻고 나갈게....",C_GREEN,2,0)
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;TARGET이 목욕을 해서 쫓겨났다
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		;自分用メモ...態度が３以上, キスかうふふ止まり, 恋慕かセフレではない, から弱みを握られてたら発生?
		IF LOCAL:1 && ARG == 5
			IF FLAG:시간정지
			ELSEIF CFLAG:318 == 1
				PRINTFORML 
			ELSE
				SELECTCASE RAND:3
					;CASE 1
						;PRINTFORML 
						;PRINTFORMW 
					;CASE 0
						;PRINTFORML 
						;PRINTFORMW 
					CASEELSE
						PRINTFORMW 「잔챙이는 비키시지! 온실 속에서 자란 화초는 얼어죽을테니까!」
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;외출처에서 조우했다
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 6
			IF FLAG:시간정지
			ELSEIF CFLAG:318 == 1
				PRINTFORML 
			;ELSEIF TALENT:연모
				;PRINTFORML 
			ELSEIF TALENT:사모
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「응, 여긴 어딜까...」
						PRINTFORMW 「뭐, 이 몸이 어딜 가든 이유 따윈 필요없지만!」
					CASE 0
						PRINTFORMW 「좋아, 개구리 {RAND:5}마리를 얼렸다고!...응?」
						CALL COLORMESSAGE14("「다이짱!다이짱도 같이 얼리지 않을래?」",C_AQUA,2,0)
						CALL COLORMESSAGE14("　아, 그게...오늘은 그러고 싶지가 않아서...",C_GREEN,2,0)
					CASEELSE
						PRINTFORML 「으음, 어떻게 돌아가야 하지?」
						CALL COLORMESSAGE14("　치르노짱, 혹시 미아가 되어버린 거야?",C_GREEN,2,0)
						PRINTFORMW 「아, 아냐! 그런 거 아니라고!」
						CALL COLORMESSAGE14("　그럼, 돌아가고 싶을 때 같이 돌아가자",C_GREEN,2,0)
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						CALL COLORMESSAGE14("　아, 치르노씨. 으앗...주변을 전부 얼려버리고 있구나",C_GREEN,2,0)
						PRINTFORML 「호수에서 나왔을 뿐인데, 이렇게나 얼릴 게 많다니! 후후훗, 다 얼려주겠어!」
						CALL COLORMESSAGE14("　치르노씨 주변의 은빛으로 물든 세상...아름다워.",C_GREEN,2,0)
						PRINTFORML 「이봐, 개구리! 다음은 네 녀석이다!」
						CALL COLORMESSAGE14("（하지만, 혼나지 않으려나?）",C_GREEN,2,0)
					CASE 0
						PRINTFORML 「읏!」
						IF ABL:MASTER:전투능력 > ABL:14:전투능력
							PRINTFORML 「이 몸을 당해내지 못할 줄 알았는데, 꽤나 잘 버티는 걸!」
							PRINTFORMW 「자, 승부야!」
							CALL COLORMESSAGE14("　자, 잠깐만!",C_GREEN,2,0)
						ELSE
							PRINTFORML 「네 녀석은 시시해」
							PRINTFORMW 「더 강한 다른 녀석을 찾아봐야겠는걸!」
						ENDIF
					CASEELSE
						PRINTFORML 「너, 너...제법인걸?」
						PRINTFORML 「그나저나, 우리 전에 어딘가에서 만났던가?」
						CALL COLORMESSAGE14("　다음번에는 잊지 말아주세요....",C_GREEN,2,0)
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;다른 캐릭터와 데이트중에 조우했다
		;ARG:1은 데이트 상대
		;기입체크 (=0, 비표시, 1, 표시)
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG == 7
			IF FLAG:시간정지
			ELSEIF CFLAG:318 == 1
				PRINTFORMW 
			;ELSEIF TALENT:연모
				;PRINTFORML 
			ELSEIF TALENT:사모
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「놀러가려고? 이 몸도 같이......핫!」
						PRINTFORMW 「오, 오늘은 이만 쿨하게 떠나주지...」
					CASE 0
						PRINTFORML 「으으...」
						CALL COLORMESSAGE14("「이봐 네놈, 다이짱을 울리면 가만 안 둘거야!」",C_AQUA,2,0)
						CALL COLORMESSAGE14("　괜찮아 치르노짱. 이 사람은 나쁘지 않으니까.",C_GREEN,2,0)
					CASEELSE
						PRINTFORMW 「휘익-휘익-! 빙정도 녹여버릴 정도로 뜨거워 보이는 걸!」
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
					CASE 1
						PRINTFORML 「재밌어 보이는 장난감 발견-!」
						PRINTFORMW 
					CASE 0
						PRINTFORML 「그렇게 꼬옥 붙어있는 채로 이 몸에게 다가왔다간 얼어서 꼼짝도 못 할걸?」
						PRINTFORMW 
					CASEELSE
						PRINTFORML 「오, 물리적으로 차갑게 얼려줄까?」
						PRINTFORMW 
				ENDSELECT
			ENDIF
			RETURN 1
		ENDIF
ENDIF
RESETCOLOR
RETURN 1

