@M_KOJO_합본판_합본판_SPEVENT_K11_3(ARG, ARG:1)
;말 주인은 TARGET
;지문 표시
CALL SPEVENT_MESSAGE_3(ARG, ARG:1)
;--------------------------------------------------
;기입 체크(=0, 비표시, 1, 표시)
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;소분기
