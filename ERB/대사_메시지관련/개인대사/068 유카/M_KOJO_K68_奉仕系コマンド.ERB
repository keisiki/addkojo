		PRINTFORMW 
		RETURN 1
	;굴복3
	ELSEIF MARK:괘씸각인 == 3
		PRINTFORMW 
		RETURN 1
	;그 외
	ELSE
		PRINTFORMW 
		RETURN 1
	ENDIF
